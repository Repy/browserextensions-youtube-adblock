{
  "manifest_version": 3,
  "version": "0.0.8",
  "minimum_chrome_version": "96",
  "author": "Repy",
  "name": "YT Ad Block",
  "description": "YT Ad Block",
  "permissions": [],
  "host_permissions": [],
  "icons": {
    "16": "icon-16.png",
    "19": "icon-24.png",
    "20": "icon-24.png",
    "24": "icon-24.png",
    "32": "icon-48.png",
    "38": "icon-48.png",
    "48": "icon-48.png",
    "120": "icon-120.png",
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "js": [
        "executeScript.js"
      ],
      "matches": [
        "*://*.youtube.com/*"
      ],
      "run_at": "document_start",
      "world": "MAIN"
    }
  ]
}